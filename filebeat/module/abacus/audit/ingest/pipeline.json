{
  "description": "Pipeline for parsing abacus audit logs",
  "processors": [
    {
      "json": {
        "field": "message",
        "target_field": "parsedMessage"
      }
    }
  ],
  "on_failure" : [{
    "set" : {
      "field" : "error.message",
      "value" : "{{ _ingest.on_failure_message }}"
    }
  }]
}
